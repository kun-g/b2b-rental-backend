{
  "name": "@b2b-rental/core",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist/main.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only --watch src dev-runner.cjs",
    "build": "tsc -p tsconfig.build.json",
    "start:prod": "node dist/main.js",
    "migrate": "ts-node --esm src/scripts/migrate.ts",
    "seed": "ts-node --esm src/scripts/seed.ts",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "dependencies": {
    "@nestjs/axios": "^3.0.0",
    "@nestjs/common": "^10.3.2",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.3.2",
    "@nestjs/platform-express": "^10.3.2",
    "@nestjs/swagger": "^7.2.2",
    "@b2b-rental/shared": "workspace:*",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0"
  }
}
